@import '../../common/common.wxss';

.form-box{
    padding-left: 30rpx;
}
.form-box input{
    height: 90rpx;
    border-bottom: 1rpx solid #ededed;
    color: #999;
}
.form-box .input-placeholder{
    color: #aaa;
}
.form-box button{
    margin:30rpx 30rpx 0 0;
}

.map_container{
    position: absolute;
    top: 38%;
    bottom: 0;
    left: 0;
    right: 0;
}
.map{
    width: 100%;
    height: 100%;
}
.map_text{
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0px;
    height: 80px;
    background: #fff;
    padding: 0 15px;
}
text{
    margin: 5px 0;
    display: block;
    font-size:12px;
}
.h1{
    margin: 15px 0;
    font-size:15px;
}